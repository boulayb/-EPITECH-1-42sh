if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
echo "(){ :|:&};:" | ./42sh
if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
echo ":(){ :|:& };:" | ./42sh
if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
echo "AEQF;bla;bli;blu;ju;jo;toto;foo|bar|baz" | ./42sh
if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
