if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
echo "Ã©" | ./42sh
