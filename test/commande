if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
echo "pwd | ls > toto | cat -e" | ./42sh
if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
echo "ls |" | ./42sh
if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
echo "ls >>> toto" | ./42sh
if [ $? -gt 129 ]
then    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
