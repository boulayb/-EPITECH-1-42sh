#!/bin/bash
rougefonce='\e[0;31m'
default='\033[39m'
echo "ls" | ./42sh
if [ $? -gt 129 ]
then
    echo -e "${rougefonce}<<<<<<<<<<!!!!!!!!!!!!!SEGMENTATION FAULT!!!!!!!>>>>>>>>>>>>>>>>>${default}"
fi
